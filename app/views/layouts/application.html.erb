<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body>
    <div class="container">
      <%= render 'layouts/navbar' %>
      <div id="flash">
        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
      </div>
    </div>
    <%= yield %>
    <%= render 'layouts/footer' %>
  </body>
</html>
